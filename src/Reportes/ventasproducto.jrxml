	<queryString language="SQL">
		<![CDATA[SELECT
     detalleventas.`iddetalleventa` AS detalleventas_iddetalleventa,
     detalleventas.`codigo` AS detalleventas_codigo,
     producto.`codigo` AS producto_codigo,
     producto.`Descripcion` AS producto_Descripcion,
     ventas.`idventa` AS ventas_idventa,
     ventas.`fecha` AS ventas_fecha,
     detalleventas.`idventa` AS detalleventas_idventa
FROM
     `producto` producto INNER JOIN `detalleventas` detalleventas ON producto.`codigo` = detalleventas.`codigo`
     INNER JOIN `ventas` ventas ON detalleventas.`idventa` = ventas.`idventa`]]>
